package hw4;

public class BabyName {
	private int rank;
	private String name;
	private int numberOfOccurrences;
	
	public BabyName(int initialRank, String initialName, int initialNumberOfOccurrences)
	{
		rank = initialRank;
		name = initialName;
		numberOfOccurrences = initialNumberOfOccurrences;
	}
	
	public int getRank()
	{
		return rank;
	}
	
	public String getName()
	{
		return name;
	}
	
	public int getNumberOfOccurrences()
	{
		return numberOfOccurrences;
	}
	
	public String toString()
	{
		return "[" + rank + "," + name + "," + numberOfOccurrences + "]";
	}
}
